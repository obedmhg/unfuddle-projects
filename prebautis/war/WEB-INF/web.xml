<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Prebautismales</servlet-name>
		<servlet-class>com.ocorp.PrebautismalesServlet</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>LogIn</servlet-name>
		<servlet-class>com.ocorp.LogIn</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>SearchRecord</servlet-name>
		<servlet-class>com.ocorp.SearchRecord</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LogIn</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SearchRecord</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Prebautismales</servlet-name>
		<url-pattern>/prebautismales</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
